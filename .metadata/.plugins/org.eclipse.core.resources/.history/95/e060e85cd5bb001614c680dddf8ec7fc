
public class JavaRobotServo extends SendUDP {
    private byte[] commandServoOn = { 89, 69, 82, 67, 32, 0, 4, 0, 3, 1, 0, 0, 0, 0, 0, 0, 57, 57, 57, 57, 57, 57, 57,
	    57, -125, 0, 2, 0, 1, 16, 0, 0, 1, 0, 0, 0 };
    private byte[] commandServoOFF = { 89, 69, 82, 67, 32, 0, 4, 0, 3, 1, 0, 0, 0, 0, 0, 0, 57, 57, 57, 57, 57, 57, 57,
	    57, -125, 0, 2, 0, 1, 16, 0, 0, 2, 0, 0, 0 };
    private Boolean status = new Boolean(false);
 // private constructor JavaRobotAlert Read
    private JavaRobotServo() {
    	super(4);//Servo ON
    }

    private JavaRobotServo(int i) {
    	super(5);//Servo OFF
    }

    public static JavaRobotServo makeServo(int index){
	switch(index){
	case 1://Servo ON
	    return new JavaRobotServo();
	case 2://Servo OFF
	    return new JavaRobotServo(123);
	default://Servo OFF
	    return new JavaRobotServo(123);
	}
    }
}
