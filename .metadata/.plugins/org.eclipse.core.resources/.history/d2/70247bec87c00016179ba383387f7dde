
public interface RobotTool {
	public int X = 0;
	public int Y = 0;
	public int Z = 0;
	public int[] robotposi = new int[3];

	public static int[] getPosition() {
		robotposi[1] = X;
		robotposi[2] = Y;
		robotposi[3] = Z;
		return robotposi;
	}
	public int getX();
	public int getY();
	public int getZ();
	public void setZ(int input);
	public void setY(int input);
	public void setX(int input);
	
}
